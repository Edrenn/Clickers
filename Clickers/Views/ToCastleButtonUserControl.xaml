<UserControl x:Class="Clickers.Views.ToCastleButtonUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Clickers.Views"
             mc:Ignorable="d" 
             d:DesignHeight="180.974" d:DesignWidth="203.944">
    <UserControl.Resources>
        <ResourceDictionary Source="../Styles/BaseDictionary.xaml"/>
    </UserControl.Resources>

    <Button x:Name="ToCastleButton" Grid.Row="1" Grid.Column="2" Style="{StaticResource MenuButton}">
        <Button.Content>
            <Grid>
                <Image Source="../Assets/Image/BackToCastle.png"/>
            </Grid>
        </Button.Content>
    </Button>
</UserControl>
